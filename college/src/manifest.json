{
    "manifest_version": 3,
    "name": "Present",
    "version": "1.0.0",
    "description": "Redirect college-admissions related sites to Rutgers",
    "icons": {
        "16": "shield16.png",
        "48": "shield48.png",
        "128": "shield128.png"
    },
    "permissions": [
        "declarativeNetRequest",
        "browsingData"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "background": {
        "service_worker": "background.js"
    }
}